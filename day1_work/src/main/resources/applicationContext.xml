<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="teacher" class="com.model.Teacher">
        <property name="name" value="张三"/>
        <property name="address" value="杭州"/>
    </bean>
    <bean id="student" class="com.model.Student">
        <property name="id" value="1"/>
        <property name="name" value="张三"/>
    </bean>

    <bean id="studentDaoMybatisImpl" class="com.model.StudentDaoMybatisImpl"/>
    <bean id="studentDaoJdbcImpl" class="com.model.StudentDaoJdbcImpl"/>
    <bean id="studentService" class="com.model.StudentService">
        <property name="studentDao" ref="studentDaoMybatisImpl"/>
    </bean>
</beans>