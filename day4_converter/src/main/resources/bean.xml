<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="person" class="com.model.Person" p:name="张三" p:birthday="2000/12/12"/>
    <bean id="localDateConverter" class="com.converter.DateConverter"/>
    <bean id="localDateConverter2" class="com.converter.DateConverter2"/>
    <bean id="conversionService" class="org.springframework.context.support.ConversionServiceFactoryBean">
        <property name="converters">
            <set>
                <ref bean="localDateConverter"/>
                <ref bean="localDateConverter2"/>
            </set>
        </property>
    </bean>
</beans>